{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "budget": {
      "type": "enumeration",
      "enum": [
        "Бюджетный",
        "Средний",
        "Дорогой"
      ],
      "required": true
    },
    "purpose": {
      "type": "enumeration",
      "enum": [
        "Для игр",
        "Для дома",
        "Для работы"
      ],
      "required": true
    },
    "rgb": {
      "type": "enumeration",
      "enum": [
        "Без подсветки",
        "С подсветкой",
        "С RGB-подсветкой"
      ],
      "required": true
    },
    "delivery": {
      "type": "enumeration",
      "enum": [
        "Быстрая",
        "Долгая"
      ],
      "required": true
    }
  }
}
